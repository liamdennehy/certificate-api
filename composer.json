{
    "name": "liamdennehy/certificate-api",
    "description": "A Certificate API and site for analysing and storing certificates",
    "keywords": ["x509", "eidas", "qcStatement", "tls", "signature"],
    "license": "GPLv3",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/liamdennehy/xmlseclibs.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/lumen-framework": "^6.0",
        "liamdennehy/eidas-certificate": ">0.6.0",
        "nyholm/psr7": "^1.2",
        "symfony/psr-http-message-bridge": "^1.2",
        "zendframework/zend-diactoros": "^2.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": ">7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
